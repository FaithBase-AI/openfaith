{
  "dependencies": {
    "@emnapi/core": "^1.4.3",
    "@emnapi/runtime": "^1.4.3",
    "@emnapi/wasi-threads": "^1.0.2",
    "@tybys/wasm-util": "^0.9.0",
    "tailwind-merge": "3.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@turbo/gen": "2.5.4",
    "@types/bun": "latest",
    "turbo": "2.5.4",
    "typescript": "5.8.3"
  },
  "name": "openfaith",
  "packageManager": "bun@1.2.13",
  "private": true,
  "resolutions": {
    "@effect/cluster": "0.42.0",
    "@effect/experimental": "0.52.0",
    "@effect/opentelemetry": "0.54.0",
    "@effect/platform": "0.88.0",
    "@effect/platform-bun": "0.73.0",
    "@effect/platform-node-shared": "0.43.0",
    "@effect/rpc": "0.65.0",
    "@effect/sql": "0.41.0",
    "@effect/sql-drizzle": "0.40.0",
    "@effect/sql-pg": "0.42.0",
    "@effect/workflow": "0.5.0",
    "effect": "3.16.13"
  },
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run format lint typecheck test --ui=stream",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:generate": "bun run --filter '@openfaith/db' db:generate",
    "db:migrate": "bun run --filter '@openfaith/db' db:migrate",
    "dev": "turbo run infra dev --continue",
    "format": "turbo run format --ui=stream --continue",
    "format:fix": "turbo run format --ui=stream --continue -- --write",
    "infra": "turbo run infra --continue",
    "lint": "turbo run lint --continue  --ui=stream -- --cache --cache-location .cache/.eslintcache",
    "lint:fix": "turbo run lint --continue --ui=stream -- --fix --cache --cache-location .cache/.eslintcache",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck --ui=stream",
    "ui-add": "turbo run ui-add"
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ],
  "type": "module",
  "workspaces": {
    "catalog": {
      "@effect/cluster": "0.42.0",
      "@effect/opentelemetry": "0.54.0",
      "@effect/platform": "0.88.0",
      "@effect/platform-bun": "0.73.0",
      "@effect/platform-node": "0.90.0",
      "@effect/rpc": "0.65.0",
      "@effect/sql-drizzle": "0.40.0",
      "@effect/sql-pg": "0.42.0",
      "@effect/workflow": "0.5.0",
      "@rocicorp/zero": "0.21.2025070200",
      "@types/qs": "6.14.0",
      "@types/react": "19.1.6",
      "@types/react-dom": "19.1.5",
      "better-auth": "1.2.12",
      "date-fns": "4.1.0",
      "drizzle-kit": "0.31.1",
      "drizzle-orm": "0.44.2",
      "effect": "3.16.13",
      "input-otp": "1.4.2",
      "jotai": "2.12.5",
      "next-themes": "0.4.6",
      "nuqs": "2.4.3",
      "qs": "6.14.0",
      "radix-ui": "1.4.2",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "resend": "4.5.1",
      "superjson": "2.2.2",
      "tailwindcss": "4.1.8",
      "typeid-js": "1.2.0",
      "typescript": "5.8.3"
    },
    "packages": [
      "adapters/*",
      "apps/*",
      "backend/*",
      "infra/*",
      "packages/*",
      "tooling/*"
    ]
  }
}
